{
  "name": "gcloud-version-validation-action",
  "version": "0.0.2",
  "private": true,
  "description": "Validates the current app engine version against existing ones",
  "main": "lib/main.js",
  "scripts": {
    "clean": "npm run clean:build && npm run clean:package",
    "clean:build": "rimraf lib",
    "clean:package": "rimraf dist",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run clean && npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toast-code/gcloud-version-validation-action.git"
  },
  "keywords": [
    "actions",
    "node"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "dotenv": "^8.2.0",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^26.0.10",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^14.10.0",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/parser": "^3.10.1",
    "@vercel/ncc": "^0.23.0",
    "eslint": "^7.8.1",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.0.1",
    "jest": "^24.9.0",
    "jest-circus": "^26.4.2",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^24.3.0",
    "typescript": "^4.0.2"
  }
}
